// Задание 4
// Напишите функцию, которая принимает массив чисел и возвращает новый массив только из отрицательных элементов массива
// ПРИМЕР: filterOnlyNegative([1, -3, -2, 7]) //[-3, -2]

function filterOnlyNegative(arrNum1) {
  const arrNum2 = [];
  for (let i = 0; i < arrNum1.length; i++) {
    if (arrNum1[i] < 0) {
      arrNum2.push(arrNum1[i]);
    }
  }
  return arrNum2;
}
console.log(filterOnlyNegative([1, -3, -2, 7]));
