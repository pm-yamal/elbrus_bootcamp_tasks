// Задание 25
// Напишите цикл от 300 до 255 с шагом 3, который вываодит "Привет!", если число кратно 5. 
// Если число одновременно кратно 9, выводить "Пока!" вместо "Привет!". Сами числа выводить не нужно.

for (let i = 300; i >= 255; i -= 3) {
  if (i % 9 === 0 && i % 5 === 0) {
    console.log('Пока!');
  } else if (i % 5 === 0) {
    console.log('Привет!');
  }
}
