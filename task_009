// Задание 9
// Считаные секунды до старта! Юрий Гагрин готов произнести свою знаменитую на весь мир фразу. 
// Напишите функцию, которая возвращает строку из чисел от 20 до 0 с шагом 4 и в конце добавляет "Поехали!".
// ПРИМЕР: cycleGo() //'201612840 Поехали!'

function cycleGo() {
  let str = '';
  for (let i = 20; i >= 0; i -= 4) {
    str += i;
  }
  return str + " Поехали!";
}
console.log(cycleGo());
