// Задание 29
// Напиши функцию, которая принимает массив строк и возвращает новый массив, содержащий только строки, длина которых больше заданного числа.
// Например: (["apple", "banana", "orange", "kiwi"], 5) → ["banana", "orange"]

function fruits(arr, num) {
  const arrNew = [];
  for (let i = 0; i < arr.length; i++) {
    if (arr[i].length > num) {
      arrNew.push(arr[i]);
    }
  } return arrNew;
} 
console.log(fruits(["apple", "banana", "orange", "kiwi"], 5));
