// Задание 40.
// Напиши функцию, которая возвращает минимальное число из двух принятых аргументов.
// Например: (1, -5) → -5.

function minNum(num1, num2) {
  if (num1 < num2) {
    return num1;
  } else {
    return num2;
  }
}
console.log(minNum(1, -5));
console.log(minNum(-7, -2));
console.log(minNum(-3, 0));
