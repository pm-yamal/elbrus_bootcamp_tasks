// Задание 33
// Напиши функцию, которая принимает число и определяет, является ли оно простым (имеет только два делителя - 1 и само число). 
// Функция должна вернуть true, если число простое, и false в противном случае.
// Например: 7 → true; 10 → false.

function simpleNum(num) {
  if (num % num === 0 && num % 1 === 0 && num % 2 !== 0 && num % 3 !== 0) {
    return true;
  }
  return false;
}
console.log(simpleNum(7));
console.log(simpleNum(10));
console.log(simpleNum(4));
console.log(simpleNum(9));
console.log(simpleNum(12));
console.log(simpleNum(11));
console.log(simpleNum(5));
